{
  "scripts": {
    "init:sqlite": "node scripts/init_sqlite_db.js",
    "dev:electron": "npm --prefix app run electron-dev",
    "dev:electron:anonymized": "SQLITE_DB_PATH=$(pwd)/dist/clarify-anonymized.sqlite USE_SQLITE=true npm --prefix app run electron-dev",
    "dev:electron:real": "SQLITE_DB_PATH=$(pwd)/dist/clarify.sqlite USE_SQLITE=true npm --prefix app run electron-dev",
    "test": "ALLOW_DEV_NO_ENCRYPTION=true npm --prefix app run test:coverage && npm --prefix renderer run test:unit && npm --prefix renderer run test:e2e",
    "icons:icns": "node scripts/generate-icns.js",
    "logs:bundle": "node scripts/collect-logs.js",
    "benchmark:check": "node scripts/benchmarks/assert-benchmarks.js"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "http-proxy-middleware": "^3.0.5"
  },
  "devDependencies": {
    "electron-icon-builder": "^2.0.1"
  }
}
